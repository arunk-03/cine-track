{
  "version": 2,
  "builds": [
    {
      "src": "src/Backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/Backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/$1"
    }
  ]
} 