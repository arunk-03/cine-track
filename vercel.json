{
  "version": 2,
  "builds": [
    {
      "src": "src/Backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/Backend/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/Backend/index.js"
    }
  ]
} 